<!doctype html>

    #parse("head/header.vm")
    #parse("head/sidebar.vm")
    #parse("head/navbar.vm")

<div class="content">
    <div class="container-fluid">
        <div class="row">
            <div class="col-md-8">
                <div class="card">
                    <div class="content">
                        <form class="form-horizontal">
                            <div class="form-group">
                                <label  class="col-sm-2 control-label">发布名称</label>
                                <div class="col-sm-10">
                                    <input type="text" class="form-control" placeholder="请输入发布名称">
                                </div>
                            </div>
                            <div class="form-group">
                                <label  class="col-sm-2 control-label">发布事件事业部</label>
                                <div class="col-sm-10">
                                    <select class="form-control">
                                        <option>请选择</option>
                                        <option>LC</option>
                                        <option>PM</option>
                                        <option>GM</option>
                                    </select>
                                </div>
                            </div>
                            <div class="form-group">
                                <label  class="col-sm-2 control-label">发布产品</label>
                                <div class="col-sm-10">
                                    <select class="form-control">
                                        <option>请选择</option>
                                        <option>运价2014</option>
                                    </select>
                                </div>
                            </div>
                            <div class="form-group">
                                <label for="inputEmail3" class="col-sm-2 control-label">发布环境</label>
                                <div class="col-sm-10">
                                    <select class="form-control">
                                        <option>请选择</option>
                                        <option>测试</option>
                                        <option>生产</option>
                                        <option>准生产</option>
                                    </select>
                                </div>
                            </div>
                            <div class="form-group">
                                <label  class="col-sm-2 control-label">发布svn地址</label>
                                <div class="col-sm-10">
                                    <input type="text" class="form-control" placeholder="请输入svn地址">
                                </div>
                            </div>
                            <div class="form-group">
                                <div class="col-sm-offset-2 col-sm-10">
                                    <button type="submit" class="btn btn-default">提交</button>
                                </div>
                            </div>
                        </form>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>



#parse("head/footer.vm")